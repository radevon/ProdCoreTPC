<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>@ViewData["Title"]</title>
        <link rel="stylesheet" href="~/libs/materialize-css/dist/css/materialize.min.css" />
        <link rel="stylesheet" href="~/dist/css/base.css" />
    </head>
    <body>
        <header>
            @{await Html.RenderPartialAsync("_Navigation");}
        </header>
       

        <!--основной контент-->
        <div id="ContentMain">
            @RenderBody()
        </div>
         
        <footer>
                <div>© @DateTime.Now.Year.ToString() - TPC STORE</div>
        </footer>
        <script src="~/libs/materialize-css/dist/js/materialize.min.js"></script>
        <script src="~/dist/js/base.js"></script>
    </body>
</html>